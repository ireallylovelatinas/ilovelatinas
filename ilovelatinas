local Material = loadstring(game:HttpGet("https://raw.githubusercontent.com/igaslight/New-PBB-UI/main/mocha"))()

-- discord webhook ( when someone launched the script )
--loadstring(game:HttpGet"https://raw.githubusercontent.com/igaslight/New-PBB-UI/main/Who's%20Using")()
-- ANTI AFK BLOCK --
loadstring(game:HttpGetAsync('https://raw.githubusercontent.com/igaslight/aid/main/anti%20afk', true))()

local UI = Material.Load({
     Title = "Mew Menu",
     Style = 3,
     SizeX = 250,
     SizeY = 330,
     Theme = "Mocha"
})

local Page = UI.New({
    Title = "Main"
})

Page.Dropdown({
    Text = "UI Theme",
    Callback = function(value)
    
    if(value == "Light") then loadstring(game:HttpGet"https://raw.githubusercontent.com/igaslight/New-PBB-UI/main/Theme%20%3A%20Light")()
    elseif(value == "Dark") then loadstring(game:HttpGet"https://raw.githubusercontent.com/igaslight/New-PBB-UI/main/Theme%20%3A%20Dark")()
    elseif(value == "Mocha") then loadstring(game:HttpGet"https://raw.githubusercontent.com/igaslight/New-PBB-UI/main/Theme%20%3A%20Mocha")()
    elseif(value == "Aqua") then loadstring(game:HttpGet"https://raw.githubusercontent.com/igaslight/New-PBB-UI/main/Theme%20%3A%20Aqua")()
    elseif(value == "Jester") then loadstring(game:HttpGet"https://raw.githubusercontent.com/igaslight/New-PBB-UI/main/Theme%20%3A%20Jester")()    
    end
    end,
    Options = {"Light", "Dark", "Mocha", "Aqua", "Jester"}
})

Page.Button({
    Text = "Pokedollar Giver",
    Callback = function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/igaslight/New-PBB-UI/main/inf%20pokedollars"))()
    end
})

Page.Button({
    Text = "Tix [+2000]",
    Callback = function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/igaslight/New-PBB-UI/main/inf%20tix"))()
    end
})

Page.Button({
    Text = "Skip Chat",
    Callback = function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/igaslight/New-PBB-UI/main/skipchat"))()
    end
})

Page.Button({
    Text = "Heal",
Callback = function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/igaslight/New-PBB-UI/main/heal"))()
end
})

Page.Button({
    Text = "Fly",
Callback = function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/igaslight/New-PBB-UI/main/fly"))()
end
})

Page.Button({
    Text = "Unstuck",
Callback = function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/igaslight/New-PBB-UI/main/unstuck"))()
end
})

Page.Toggle({
    Text = "Repel",
    Callback = function(value)
       local _p = nil
        for _, v in pairs(getgc(true)) do
           if typeof(v) == "table" then
               if rawget(v, "PlayerData") then
                   _p = v
                   break
               end
           end
        end
        if value then _p.Repel.steps = 1000000
        else
        _p.Repel.steps = 0
    end
    Enabled = false
    end
})

Page.Button({
    Text = "Clip Fly ['X' - Toggle]",
Callback = function()
loadstring(game:HttpGetAsync('https://raw.githubusercontent.com/igaslight/New-PBB-UI/main/Universal%20Fly', true))()
end
})

local Page2 = UI.New({
    Title = "Breeding"
})

UI.Banner({
    Text = "Some buttons are location-sensitive, and some buttons function poorly. Please be sure to save after every action you perform. For example: save after every use of this gui's FLY button.                                                                          Thank you for using my script <3"
})

Page2.Button({
    Text = "Hoverboard Spin ['M' - Toggle]",
Callback = function()
local uis = game:GetService('UserInputService');
local vim = game:GetService("VirtualInputManager")
local bind = Enum.KeyCode.M;
uis.InputBegan:connect(function(key)
    if key.KeyCode == bind then
      vim:SendKeyEvent(true, "W", false, nil) vim:SendKeyEvent(true, "A", false, nil)
    end;
    end)
end
})

Page2.Button({
    Text = "Bring Daycare Man",
Callback = function()
local lp = game:service'Players'.LocalPlayer;
game:GetService("Workspace").chunk9["DayCareMan"]:MoveTo(lp.Character.LeftHand.Position)
local Object = game.ReplicatedStorage.DayCare:WaitForChild("DayCareLady")
local CloneObject = Object:Clone()
CloneObject.Parent = game.Workspace.chunk9
game:GetService("Workspace").chunk9["DayCareLady"]:MoveTo(lp.Character.LeftHand.Position)
end
})

Page2.Button({
    Text = "PC - Lagoona",
    Callback = function()
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(312, 110, -1066)
end
})

local Page3 = UI.New({
    Title = "Misc"
})

Page3.TextField({
    Text = "Player Card Name",
    Callback = function(value)
        local _p = nil
        for _, v in pairs(getgc(true)) do
           if typeof(v) == "table" then
               if rawget(v, "PlayerData") then
                   _p = v
                   break
               end
           end
        end
    _p.Network:post('PDS', 'chooseName', value)
    end
})

Page3.Slider({
    Text = "Fps Control",
    Callback = function(value)
    pcall(setfpscap, value)
    pcall(set_fps_cap, value)
    end,
    Min = 15,
    Max = 300,
    Def = 60
})

Page3.Button({
    Text = "Fps Booster",
Callback = function()
loadstring(game:HttpGetAsync('https://raw.githubusercontent.com/igaslight/aid/main/fps%20booster', true))()
end
})

Page3.TextField({
    Text = "Chat Logger - webhook here",
    Callback = function(value)
local marketplaceService = game:GetService("MarketplaceService")

local isSuccessful, info = pcall(marketplaceService.GetProductInfo, marketplaceService, Game.PlaceId)
if isSuccessful then

local wh = value
local embed1 = {
       ['title'] = 'Beginning of Message logs on '..info.Name.." at "..tostring(os.date("%m/%d/%y at time %X"))
   }
local a = syn.request({
   Url = wh,
   Headers = {['Content-Type'] = 'application/json'},
   Body = game:GetService("HttpService"):JSONEncode({['embeds'] = {embed1}, ['content'] = ''}),
   Method = "POST"
})
function logMsg(Webhook, Player, Message)
   local embed = {
       ['description'] = Player..": "..Message.."  " ..tostring(os.date("| time %X")) 
   }
   local a = syn.request({
       Url = Webhook,
       Headers = {['Content-Type'] = 'application/json'},
       Body = game:GetService("HttpService"):JSONEncode({['embeds'] = {embed}, ['content'] = ''}),
       Method = "POST"
   })
end
for i,v in pairs(game.Players:GetPlayers()) do
   v.Chatted:Connect(function(msg)
       logMsg(wh, v.Name, msg)
   end)
end

game.Players.PlayerAdded:Connect(function(plr)
   plr.Chatted:Connect(function(msg)
       logMsg(wh, plr.Name, msg)
   end)
end)
end
    end
})


