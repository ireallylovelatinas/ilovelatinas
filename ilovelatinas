local UI = Material.Load({
     Title = "Pokemon Cheat Menu",
     Style = 3,
     SizeX = 250,
     SizeY = 330,
     Theme = "Mocha"
})

local Page = UI.New({
    Title = "Main"
})


Page.Dropdown({
    Text = "UI Theme",
    Callback = function(value)
    
    if(value == "Light") then loadstring(game:HttpGet"https://raw.githubusercontent.com/discordxj2shn0001/New-PBB-UI/main/Theme%20%3A%20Light")()
    elseif(value == "Dark") then loadstring(game:HttpGet"https://raw.githubusercontent.com/discordxj2shn0001/New-PBB-UI/main/Theme%20%3A%20Dark")()
    elseif(value == "Mocha") then loadstring(game:HttpGet"https://raw.githubusercontent.com/discordxj2shn0001/New-PBB-UI/main/Theme%20%3A%20Mocha")()
    elseif(value == "Aqua") then loadstring(game:HttpGet"https://raw.githubusercontent.com/discordxj2shn0001/New-PBB-UI/main/Theme%20%3A%20Aqua")()
    elseif(value == "Jester") then loadstring(game:HttpGet"https://raw.githubusercontent.com/discordxj2shn0001/New-PBB-UI/main/Theme%20%3A%20Jester")()    
    end
    
    end,
    Options = {"Light", "Dark", "Mocha", "Aqua", "Jester"}
})

Page.Button({
    Text = "Pokedollar Giver",
    Callback = function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/igaslight/New-PBB-UI/main/inf%20pokedollars"))()
    end
})

Page.Button({
    Text = "Tix [+2000]",
    Callback = function()
		local _p = nil
        for _, v in pairs(getgc(true)) do
           if typeof(v) == "table" then
               if rawget(v, "Events") then
                   _p = v
                   break
               end
           end
        end
_p.Network:get("PDS", "AlolanReward",1000)
    end
})

