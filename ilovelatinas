local Page2 = UI.New({
    Title = "Breeding"
})

UI.Banner({
    Text = "Some buttons are location-sensitive, and some buttons function poorly. Please be sure to save after every action you perform. For example: save after every use of this gui's FLY button.                                                                          Thank you for using my script <3"
})

Page2.Button({
    Text = "Hoverboard Spin ['M' - Toggle]",
Callback = function()
local uis = game:GetService('UserInputService');
local vim = game:GetService("VirtualInputManager")
local bind = Enum.KeyCode.M;


uis.InputBegan:connect(function(key)
    if key.KeyCode == bind then
        --Key Script
      
        
        vim:SendKeyEvent(true, "W", false, nil) vim:SendKeyEvent(true, "A", false, nil)

        
    end;
    end)
end
})

Page2.Button({
    Text = "Instant Hatch {Testing}",
Callback = function()
   local _p = nil
        for _, v in pairs(getgc(true)) do
           if typeof(v) == "table" then
               if rawget(v, "PlayerData") then
                   _p = v
                   break
               end
           end
        end 
_p.Network:get('PDS', 'completedEggCycle')    
    
end
})

